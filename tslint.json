{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "eofline": false,
        "quotemark": [true, "single", "avoid-escape"],
        "trailing-comma": [true, {
            "multiline": "never",
            "singleline": "never"
        }],
        "curly": [true, "ignore-same-line"]
    },
    "rulesDirectory": []
}